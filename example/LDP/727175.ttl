@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix courses:  <http://localhost:3000/univ/umaine/courses/> .
@prefix univ:  <http://localhost:3000/univ/> .

# There's no URI for the Student in this case because
# We're POSTing this to an LDPC to create it. The URI
# will be provided through the LDPC and Slug header

<>
	a univ:Student ;
	dcterms:title "Fred Johnson" ;
	univ:age  33 .
	univ:takes  courses:CS101, courses:ME201, courses:EN100 .




